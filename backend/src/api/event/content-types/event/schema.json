{
  "kind": "collectionType",
  "collectionName": "events",
  "info": {
    "singularName": "event",
    "pluralName": "events",
    "displayName": "Event",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "slug": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "description": {
      "type": "blocks"
    },
    "price": {
      "type": "decimal",
      "required": true
    },
    "date": {
      "type": "datetime"
    },
    "cover_image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "promoter": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::promoter.promoter",
      "inversedBy": "events"
    },
    "meta_title": {
      "type": "string",
      "required": true
    },
    "meta_description": {
      "type": "text"
    }
  }
}
