    },
    "promoter": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::promoter.promoter"
    },
    "venue": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::venue.venue"
    },
    "bouts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::bout.bout"
    },
    "fighter": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::fighter.fighter",
      "inversedBy": "events"
    }
